  } else {
    return;  
  }
  
  if(player.y>265){
   // player.y = 10;
   // player.y = 220;
  } else if(player.y>170 && player.y<265){
    player.x = 202; 
    player.y = 285;
  } else if(player.y>96&& player.y<170){
    player.x = 202; 
    player.y = 220;
  } else if(player.y<96){
    player.x = 202; 
    player.y = 155;
  } 
}


function room_LeaveRight()
{
  if(delay_goedge==null){
